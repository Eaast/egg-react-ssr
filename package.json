{
  "name": "egg-react-ssr",
  "version": "1.0.0",
  "description": "采用egg+mysql+sequelize+react+redux+redux-thunk+antd搭建的服务端渲染框架",
  "private": true,
  "egg": {
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-egg-react-ssr",
    "stop": "egg-scripts stop --title=egg-server-egg-react-ssr",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "ci": "npm run lint && npm run cov",
    "autod": "autod",
    "clean": "rimraf ./dist",
    "build-react": "npm run clean && cross-env NODE_ENV=production npm-run-all --parallel build:react**",
    "build:react-server": "webpack --config ./react-ssr/build/webpack.server.js",
    "build:react-client": "webpack --config ./react-ssr/build/webpack.client.js",
    "dev-react": "npm run clean && cross-env NODE_ENV=development npm-run-all --parallel dev:react**",
    "dev:react-client": "webpack-dev-server --config ./react-ssr/build/webpack.client.js",
    "dev:react-server": "webpack --config ./react-ssr/build/webpack.server.js --watch",
    "lint": "eslint --ext .jsx,.js app/ config/ react-ssr/",
    "lint:fix": "eslint --fix --ext .js,.jsx .",
    "stylelint": "stylelint 'react-ssr/**/*.(less|css)' --fix",
    "precommit": "lint-staged"
  },

  "dependencies": {
    "@ant-design/icons": "^4.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@loadable/babel-plugin": "^5.12.0",
    "@loadable/component": "^5.12.0",
    "@loadable/server": "^5.12.0",
    "@loadable/webpack-plugin": "^5.12.0",
    "antd": "^4.0.2",
    "autoprefixer": "^9.7.4",
    "axios": "^0.21.1",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.0",
    "classnames": "^2.2.6",
    "core-js": "^3.6.4",
    "css-loader": "^3.4.2",
    "egg": "^2.15.1",
    "egg-http-proxy": "^1.0.1",
    "egg-jwt": "^3.1.7",
    "egg-mysql": "^3.0.0",
    "egg-scripts": "^2.13.0",
    "egg-validate": "^2.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^4.5.1",
    "husky": "^4.3.8",
    "is-wsl": "^2.1.1",
    "memory-fs": "^0.5.0",
    "mini-css-extract-plugin": "^0.9.0",
    "moment": "^2.29.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.2.1",
    "qs": "^6.9.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "rimraf": "^3.0.2",
    "sqlite3": "latest",
    "style-loader": "^1.1.3",
    "stylelint": "^13.3.3",
    "stylelint-order": "^4.1.0",
    "terser-webpack-plugin": "^2.3.5",
    "url-loader": "^3.0.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^5.7.3",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.10.1",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "babel-loader": "^8.0.6",
    "cross-env": "^7.0.3",
    "css-hot-loader": "^1.4.4",
    "egg-bin": "^4.15.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "eslint-config-egg": "^7.1.0",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
    "webpack-dev-server": "^3.11.2",
    "lint-staged": "^10.5.3"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "ci": {
    "version": "10"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "lzc",
  "license": "MIT"
}
